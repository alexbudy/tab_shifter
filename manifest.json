{
  "name": "Tab Shifter",
  "version": "1.7",
  "permissions": [
    "tabs", "background", "storage"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["inject.js"]
    }],
  "background" : {
      "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "manifest_version": 2
}
