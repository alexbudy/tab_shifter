{
  "name": "Tab Shifter",
  "version": "1.11",
  "permissions": [
    "tabs", "background", "storage"
  ],
  "icons": {
    "16"  : "icon16.png",
    "48"  : "icon48.png",
    "128" : "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["inject.js"]
    }],
  "background" : {
      "scripts": ["background.js"]
  },
  "options_page": "options.html",
  "manifest_version": 2
}
